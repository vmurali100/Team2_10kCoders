<body>
    <button onclick="addSkills()">add skill</button>
    <button onclick="scriptType()">type of script</button>

    <script>
        var person = {
            name: "tejdeep",
            age: 22,
            exp: null,

        }
        console.log(person)
        function addSkills() {
            person.skill = "ui developer"

            console.log(person)
        }
        function scriptType() {
            person.type = "javascript"
            console.log(person)
        }
        function gender() {
            person.gender = "male"
            console.log(person)
        }
    </script>
